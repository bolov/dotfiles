# Personal account, - the default config
Host gitlab.com
   HostName gitlab.com
   User git
   IdentityFile ~/.ssh/id_rsa

# Holosee account
Host holosee.gitlab.com
   HostName gitlab.com
   User git
   IdentityFile ~/.ssh/id_rsa_holosee

# Holosee AWS
Host *.amazonaws.com
   User mihail.balan
   IdentityFile ~/.ssh/id_rsa_holosee

# EC2
Host dev1.holosee.aws
    HostName ec2-52-28-152-185.eu-central-1.compute.amazonaws.com
    IdentityFile ~/.ssh/id_rsa_holosee

# EC2
Host testbed1.holosee.aws
    HostName ec2-3-125-108-128.eu-central-1.compute.amazonaws.com
    IdentityFile ~/.ssh/id_rsa_holosee
